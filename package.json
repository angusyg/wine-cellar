{
  "name": "wine-cellar",
  "version": "1.0.0",
  "description": "Wine cellar management application",
  "main": "./backend/bin/www",
  "scripts": {
    "test": "mocha test/tests.js --timeout 10000 --exit",
    "dev": "concurrently \"npm run dev:back\" \"npm run dev:front\"",
    "dev:back": "cross-env NODE_ENV=development cross-env DEBUG=nean:* node ./node_modules/nodemon/bin/nodemon",
    "dev:front": "cross-env NODE_ENV=development node ./node_modules/gulp/bin/gulp",
    "start": "node ./backend/bin/www",
    "build": "cross-env NODE_ENV=production node ./node_modules/gulp/bin/gulp",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/angusyg/wine-cellar.git"
  },
  "keywords": [
    "nodejs",
    "angularjs",
    "express",
    "nedb"
  ],
  "author": "angusyg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angusyg/wine-cellar/issues"
  },
  "homepage": "https://github.com/angusyg/wine-cellar#readme",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.15",
    "angular": "^1.6.9",
    "angular-animate": "^1.6.9",
    "angular-messages": "^1.6.9",
    "angular-storage": "0.0.15",
    "angular-translate": "^2.17.0",
    "angular-translate-loader-partial": "^2.17.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "camo": "^0.12.3",
    "compression": "^1.7.2",
    "continuation-local-storage": "^3.2.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.6",
    "csurf": "^1.9.0",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-pino-logger": "^3.0.1",
    "helmet": "^3.12.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.2.0",
    "kind-of": "^6.0.2",
    "pino": "^4.14.0",
    "pino-debug": "^1.1.1",
    "pino-multi-stream": "^3.1.2",
    "ui-bootstrap4": "^3.0.2",
    "uuid": "^3.2.1"
